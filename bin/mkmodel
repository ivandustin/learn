#!/usr/bin/env python
from learn import DIMS, CLASSES, NEURONS, MODEL, checkpointer
from learn.model.create import create
from learn.state.save import save
from learn.txt.read import read

dims = read(DIMS)
classes = read(CLASSES)
neurons = read(NEURONS)
model = create(dims, classes, neurons)
save(MODEL, model)
checkpointer.close()
